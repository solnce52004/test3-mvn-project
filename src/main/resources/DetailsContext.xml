<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="headBean" class="dev.example.beans.details.Head"
          p:detailName="head"
          p:action="spin"
          scope="prototype">
        <qualifier value="headSpin" />
    </bean>

    <bean id="legBeanGo" class="dev.example.beans.details.Leg"
          p:detailName="leg"
          p:action="go"
          scope="prototype">
        <qualifier value="legGo"/>
    </bean>

    <bean id="legBeanStop" class="dev.example.beans.details.Leg"
          p:detailName="leg"
          p:action="stop"
          scope="prototype">
        <qualifier value="legStop" />
    </bean>
</beans>